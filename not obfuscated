--// Prompt for key input
local player = game.Players.LocalPlayer
local inputGui = Instance.new("ScreenGui")
inputGui.Name = "KeyInputGui"
inputGui.ResetOnSpawn = false
inputGui.Parent = player:WaitForChild("PlayerGui")

local inputFrame = Instance.new("Frame")
inputFrame.Size = UDim2.new(0, 300, 0, 150)
inputFrame.Position = UDim2.new(0.5, -150, 0.5, -75)
inputFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
inputFrame.BackgroundTransparency = 0.4
inputFrame.Parent = inputGui

local corner = Instance.new("UICorner", inputFrame)
corner.CornerRadius = UDim.new(0, 12)

local stroke = Instance.new("UIStroke", inputFrame)
stroke.Thickness = 2
stroke.Color = Color3.fromRGB(255, 255, 255)

local label = Instance.new("TextLabel", inputFrame)
label.Size = UDim2.new(1, 0, 0, 40)
label.Position = UDim2.new(0, 0, 0, 10)
label.BackgroundTransparency = 1
label.Text = "Enter Key to Access"
label.TextColor3 = Color3.new(1, 1, 1)
label.Font = Enum.Font.GothamBold
label.TextScaled = true

local textbox = Instance.new("TextBox", inputFrame)
textbox.PlaceholderText = "Enter Key..."
textbox.Size = UDim2.new(0.8, 0, 0, 36)
textbox.Position = UDim2.new(0.1, 0, 0, 60)
textbox.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
textbox.TextColor3 = Color3.fromRGB(255, 255, 255)
textbox.ClearTextOnFocus = false
textbox.TextScaled = true
textbox.Font = Enum.Font.Gotham
Instance.new("UICorner", textbox)

local submitBtn = Instance.new("TextButton", inputFrame)
submitBtn.Size = UDim2.new(0.5, 0, 0, 30)
submitBtn.Position = UDim2.new(0.25, 0, 1, -40)
submitBtn.Text = "Submit"
submitBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
submitBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
submitBtn.Font = Enum.Font.GothamBold
submitBtn.TextScaled = true
Instance.new("UICorner", submitBtn)

submitBtn.MouseButton1Click:Connect(function()
    if textbox.Text == CORRECT_KEY then
        inputGui:Destroy()
        runMainScript()
    else
        textbox.Text = ""
        label.Text = ":x: Incorrect Key"
        wait(1)
        label.Text = "Enter Key to Access"
    end
end)

    -- [ Insert your entire script content here ‚Äî everything you shared above ]
    -- For brevity, I'll stop repeating it in this reply. You should paste your full code here.

end

